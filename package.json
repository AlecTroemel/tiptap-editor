{
    "name": "tiptap-editor",
    "version": "1.1.3",
    "description": "custom setup for the tiptap editor",
    "repository": "https://github.com/mirusresearch/tiptap-editor",
    "bugs": {
        "url": "https://github.com/mirusresearch/tiptap-editor"
    },
    "homepage": "https://github.com/mirusresearch/tiptap-editor",
    "author": "alec@mirusresearch.com",
    "license": "MIT",
    "main": "dist/tiptap-editor.umd.min.js",
    "browser": {
        "./sfc": "src/tiptap-editor.vue"
    },
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "*.json",
        "*.js"
    ],
    "scripts": {
        "dev": "yarn serve",
        "serve": "vue serve src/dev.vue",
        "build": "WEBPACK_TARGET=node vue-cli-service build --target lib --name tiptap-editor src/tiptap-editor.vue"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/polyfill": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@vue/babel-preset-app": "^3.5.1",
        "@vue/cli-plugin-babel": "^3.5.5",
        "@vue/cli-service": "^3.5.1",
        "babel-loader": "^8.0.5",
        "current-script-polyfill": "^1.0.0",
        "eslint": "^5.15.3",
        "lodash.get": "^4.4.2",
        "lodash.unescape": "^4.0.1",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "tippy.js": "^3.4.1",
        "tiptap": "1.8.0",
        "tiptap-commands": "1.4.0",
        "tiptap-extensions": "1.8.0",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.29.6",
        "webpack-bundle-analyzer": "^3.1.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "dependencies": {
        "es7-object-polyfill": "^0.0.7"
    }
}
