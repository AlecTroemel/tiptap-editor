        "@tiptap/vue-2": "^2.1.10",
        "@vitejs/plugin-vue2": "^2.2.0",
        "@vitest/coverage-v8": "^0.32.2",
        "@vue/babel-preset-app": "^3.7.0",
        "@vue/test-utils": "^1.3.6",
        "babel-loader": "^8.0.5",
        "babel-polyfill": "^6.26.0",
        "current-script-polyfill": "^1.0.0",
        "eslint": "^5.15.3",
        "flush-promises": "^1.0.2",
        "happy-dom": "^9.20.3",
        "lodash.get": "^4.4.2",
        "lodash.unescape": "^4.0.1",
        "prettier": "2.8.8",
        "sass": "1.60.0",
        "tippy.js": "^4.3.1",
        "vite": "^4.3.9",
        "vitest": "^0.32.2",
        "vue": "^2.6.10",
        "webpack": "^4.31.0"
