{
    "name": "@mirus-private/tiptap-editor",
    "version": "1.0.28",
    "description": "custom setup for the tiptap editor",
    "main": "dist/tiptop-editor.umd.js",
    "module": "dist/tiptop-editor.esm.js",
    "unpkg": "dist/tiptop-editor.min.js",
    "repository": "https://gitlab.mirus.io/root/node_modules/tiptap-editor",
    "author": "alec@mirusresearch.com",
    "license": "MIT",
    "browser": {
        "./sfc": "src/tiptop-editor.vue"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "scripts": {
        "dev": "yarn serve",
        "serve": "vue serve src/dev.vue",
        "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
        "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd --file dist/tiptop-editor.umd.js",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es --file dist/tiptop-editor.esm.js",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife --file dist/tiptop-editor.min.js"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@vue/cli": "^3.3.0",
        "@vue/cli-service-global": "^3.3.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.15.3",
        "graphql": "^14.1.1",
        "minimist": "^1.2.0",
        "node-sass": "^4.11.0",
        "postcss": "^7.0.14",
        "rollup": "^1.1.2",
        "rollup-plugin-buble": "^0.19.6",
        "rollup-plugin-commonjs": "^9.2.0",
        "rollup-plugin-css-only": "^1.0.0",
        "rollup-plugin-replace": "^2.1.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "rollup-plugin-vue": "^4.6.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.22",
        "vue-template-compiler": "^2.5.22",
        "webpack": "^4.29.6"
    },
    "dependencies": {
        "lodash.get": "^4.4.2",
        "prosemirror-model": "^1.7.0",
        "prosemirror-state": "^1.2.2",
        "prosemirror-tables": "^0.7.10",
        "prosemirror-utils": "^0.7.7",
        "tippy.js": "^3.4.1",
        "tiptap": "^1.8.0",
        "tiptap-extensions": "^1.8.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
